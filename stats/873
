    <script type="text/javascript">
  	//<![CDATA[
  	
  		var showBlockUI = true;
  	
        function showWaitLabel() {
            $.blockUI({ message: $('#waitLabel') });
        }
        function hideWaitLabel() {
            $.unblockUI;
        }

        function waitLabelManageOnAjax(data) {
            waitLabelManageOnAjaxHelper(data, true);
        }
        
        function waitLabelManageOnAjaxNoScroll(data) {
            waitLabelManageOnAjaxHelper(data, false);
        }

        function blockUI() {
            if (showBlockUI) PF('uiBlocker').block();
        }

        function unblockUI() {
            PF('uiBlocker').unblock();
        }

        function waitLabelManageOnAjaxHelper(data, withScroll) {
            if (data.status == "begin") {
                showblockUI = true;
                setTimeout(blockUI, 400);
            } 
            else if (data.status == "complete"){
            	showBlockUI = false;
            	unblockUI();
            } else if (data.status == "success") {
                if (withScroll) {
            		scrollToError();
                }
            }
        }
        function scrollToError() {
            var el = $("#resume");
        	if ((el.length) && (el.offset() != undefined) && (el.offset().top != undefined)) {
            	$('html, body').animate({
                	scrollTop: el.offset().top
            	}, 1000);
        	}
        }
    //]]>
    </script>
